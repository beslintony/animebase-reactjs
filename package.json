{
  "name": "animebase-reactjs",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "schema:download": "npx apollo service:download --endpoint=https://anilist.co/graphql ./src/graphql/graphql-schema.json",
    "schema:gen": "npx apollo codegen:generate --localSchemaFile=./src/graphql/graphql-schema.json --globalTypesFile=./src/graphql/globalTypesFile.ts --target=typescript --tagName=gql --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.4.14",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.1",
    "@mui/styled-engine-sc": "^5.0.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/react-router-dom": "^5.3.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-is": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux-logger": "^3.0.6",
    "reselect": "^4.0.0",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vitejs/plugin-react": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  }
}
